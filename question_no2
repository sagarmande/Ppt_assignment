def removeElement(nums, val):
    i = 0  # pointer for the new array
    for j in range(len(nums)):  # pointer for the original array
        if nums[j] != val:
            nums[i] = nums[j]  # overwrite the element at index i with the element at index j
            i += 1
    return i

#nums = [3, 2, 2, 3]
val = 3
